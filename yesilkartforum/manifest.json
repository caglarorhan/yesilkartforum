{
  "name": "yesilkartforum",
  "description" : "Some tools for yesilkartforum.com web page",
  "version": "2019.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "YesilKartForum",
    "default_icon": "./img/yesilkartforum.png"
  },
  "permissions": ["declarativeContent","storage","notifications"],
  "icons": {
    "16": "./img/yesilkartforum_16.png",
    "32": "./img/yesilkartforum_32.png",
    "64": "./img/yesilkartforum_64.png",
    "128": "./img/yesilkartforum_128.png",
    "256": "./img/yesilkartforum_256.png"
  },
  "background" : { "scripts" : ["./js/background.js"] },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Some tools for yesilkartforum.com web page"
    }
  },
  "options_page" : "options.html"

}
